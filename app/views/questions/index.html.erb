<div class="generator-page">
  <%= render 'shared/navbar' %>
  <%=  form_with url: generate_path, html: {class:"generator-flex"} do |f| %>

      <div class="questions-display">
        <% @questions_and_htmls.each do |question_and_html| %>
          <div class="question">
            <%= check_box_tag "question_id[]", question_and_html[:question][:id] %>
            <%= question_and_html[:html].html_safe %>
          </div>
        <% end %>
      </div>

      <div>
          <%= f.submit "Submit" %>
      </div>

  <% end %>
</div>
